package appication;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		int i = 0;
		int j = 0;
		int[][] mat = new int [n][n];
		
		for( i=0 ; i<n ; i++) {
			for( j=0 ; j<n; j++) {
				mat[i][j] = sc.nextInt();
			}
			System.out.println(mat[i][j]);
		}
		System.out.println("Main diagonal: ");
		for ( i = 0 ; i<n; i++) {
			System.out.print(mat[i][i]+ " ");
			
		}
		
		sc.close();
	}

}
